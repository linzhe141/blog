{
  "name": "linzhe-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "prisma generate",
    "build:tw": "tailwindcss -i ./app/globals.css -o ./public/output.css --config ./tailwind.cdn.config.mts",
    "dev": "dotenv -e .env.dev.local -- next dev",
    "build": "prisma generate && next build && npm run build:tw",
    "start": "next start",
    "lint": "next lint",
    "prettier-lint": "prettier --write .",
    "migrate:deploy": "dotenv -e .env.local -- prisma migrate deploy",
    "migrate:dev": "dotenv -e .env.dev.local -- prisma migrate dev --name=dev",
    "migrate:reset": "dotenv -e .env.dev.local -- prisma migrate reset",
    "gen-posts-info": "node script/gen-posts-info.mjs"
  },
  "dependencies": {
    "@mdx-js/loader": "^2.3.0",
    "@mdx-js/react": "^2.3.0",
    "@monaco-editor/react": "^4.6.0",
    "@next/mdx": "^13.5.4",
    "@prisma/client": "^6.6.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@shikijs/core": "^3.3.0",
    "@tailwindcss/typography": "^0.5.10",
    "@types/fs-extra": "^11.0.2",
    "@types/mdx": "^2.0.8",
    "@types/node": "20.5.6",
    "@types/react": "18.2.24",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "10.4.15",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "8.47.0",
    "eslint-config-next": "13.4.19",
    "eslint-config-prettier": "^9.0.0",
    "fast-glob": "^3.3.3",
    "framer-motion": "^10.16.4",
    "fs-extra": "^11.1.1",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.9.0",
    "immer": "^10.0.3",
    "lucide-react": "^0.373.0",
    "medium-zoom": "^1.1.0",
    "monaco-editor": "^0.46.0",
    "next": "14.2.35",
    "next-mdx-remote": "^4.4.1",
    "next-themes": "^0.2.1",
    "next13-progressbar": "^1.0.4",
    "postcss": "8.4.28",
    "prettier": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "prisma": "^6.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-loading-skeleton": "^3.3.1",
    "rehype-mdx-code-props": "^2.0.0",
    "remark": "^14.0.3",
    "remark-gfm": "3.0.1",
    "remark-slug": "^7.0.1",
    "shiki": "^3.3.0",
    "tailwind-merge": "^2.2.1",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.2.2",
    "use-immer": "^0.9.0",
    "zustand": "^4.4.3"
  },
  "pnpm": {
    "ignoredBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "prisma"
    ],
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "prisma"
    ]
  }
}
