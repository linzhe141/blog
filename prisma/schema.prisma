// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Menu {
  id       Int     @id() @default(autoincrement())
  label    String  @unique
  linked   Boolean @default(false)
  url      String  @unique
  // filePath String  @unique
  name     String  @unique
  parentId Int
}

model RequsetKey {
  id  Int    @id @default(autoincrement())
  key String @unique
}
